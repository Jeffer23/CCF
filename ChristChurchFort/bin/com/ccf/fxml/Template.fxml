<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>


<VBox fx:controller="com.ccf.controller.MenuController" maxHeight="-Infinity"
	maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
	prefHeight="700.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/8"
	xmlns:fx="http://javafx.com/fxml/1">
	<children>
		<MenuBar>
			<menus>
				<Menu mnemonicParsing="false" text="Family">
					<items>
						<MenuItem mnemonicParsing="false" text="Add New Family"
							onAction="#addNewFamily" />
						<MenuItem mnemonicParsing="false" text="Edit Family"
							onAction="#editFamily" />
						<MenuItem mnemonicParsing="false" text="Exit" onAction="#exit" />
					</items>
				</Menu>
				<Menu mnemonicParsing="false" text="Dashboard">
					<items>
						<MenuItem mnemonicParsing="false" text="Comparison Report"
							onAction="#showComparisonPage" />
						<MenuItem mnemonicParsing="false" text="Month Report"
							onAction="#showMonthReportPage" />
					</items>
				</Menu>
				<Menu mnemonicParsing="false" text="Reports">
					<items>
						<MenuItem mnemonicParsing="false" text="Birthday"
							onAction="#showBirthdayReportPage" />
						<MenuItem mnemonicParsing="false" text="Marriage"
							onAction="#showMarriageReportPage" />
						<MenuItem mnemonicParsing="false" text="Santha" onAction="#showReportPage" />
						<MenuItem mnemonicParsing="false" text="Member Santha"
							onAction="#showMemberSanthaReport" />
					</items>
				</Menu>
				<Menu mnemonicParsing="false" text="Offerings">
					<items>
						<MenuItem mnemonicParsing="false" text="Service"
							onAction="#payServiceOffering" />
						<MenuItem mnemonicParsing="false" text="Santha" onAction="#paySantha" />
					</items>
				</Menu>
				<Menu mnemonicParsing="false" text="Accounts">
					<items>
						<MenuItem mnemonicParsing="false" text="Add Income"
							onAction="#showAddIncomePage" />
						<MenuItem mnemonicParsing="false" text="Add Expense"
							onAction="#loadExpensePage" />
					</items>
				</Menu>
				<Menu mnemonicParsing="false" text="Statements">
					<items>
						<MenuItem mnemonicParsing="false" text="Account Details"
							onAction="#loadAccountDetails" />
						<MenuItem mnemonicParsing="false" text="Account Statement"
							onAction="#loadAccountStatement" />
						<MenuItem mnemonicParsing="false" text="Accumulated Account Statement"
							onAction="#loadAccumulatedAccountStatement" />

					</items>
				</Menu>
			</menus>
		</MenuBar>
		<AnchorPane fx:id="body" prefHeight="777.0" prefWidth="1280.0"
			style="-fx-background-image: url('com/ccf/images/background.gif');-fx-background-repeat:repeat;" />
	</children>
</VBox>
