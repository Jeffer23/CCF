<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping SYSTEM "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ccf.persistence.classes.Ledger" table="ledger">
		<meta attribute="class-description">
			This class contains the Ledger detail.
		</meta>

		<id name="ledgerId" type="int" column="ledger_id">
			<generator class="native" />
		</id>
		<property name="ledgerName" column="ledger_name" type="string" />
		<set name="bankGraveyardAccounts" table="bank_graveyard_account" 
				inverse="true" lazy="true" fetch="subselect">
            <key>
                <column name="no" not-null="true" />
            </key>
            <one-to-many class="com.ccf.persistence.classes.BankGraveyardAccount" />
        </set>
        <set name="bankMensAccounts" table="bank_mens_account" 
				inverse="true" lazy="true" fetch="subselect">
