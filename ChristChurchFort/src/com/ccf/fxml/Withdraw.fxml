<?xml version="1.0" encoding="UTF-8"?>

<?import eu.schudt.javafx.controls.calendar.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.cell.*?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
	minWidth="-Infinity" prefHeight="800.0" prefWidth="1280.0"
	xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="com.ccf.controller.WithdrawController">
	<children>
		<AnchorPane prefHeight="775.0" prefWidth="1280.0">
			<children>
				<ImageView fitHeight="100.0" fitWidth="100.0" layoutX="1047.0"
					pickOnBounds="true" preserveRatio="true"
					style="-fx-box-shadow: 10px 10px 5px #888888;">
					<image>
						<Image url="/com/ccf/images/logo.png" />
					</image>
				</ImageView>
				<Button layoutX="651.0" layoutY="330.0" mnemonicParsing="false"
					onAction="#withdraw" prefHeight="22.0" prefWidth="91.0"
					style="-fx-background-color: #0077cc; -fx-background-radius: 15;"
					text="Withdraw" textFill="WHITE">
					<font>
						<Font name="Arial Rounded MT Bold" size="14.0" />
					</font>
					<cursor>
						<Cursor fx:constant="HAND" />
					</cursor>
				</Button>
				<Label fx:id="message" alignment="CENTER" layoutX="206.0"
					layoutY="368.0" prefHeight="25.0" prefWidth="822.0" textAlignment="CENTER"
					textFill="GREEN">
					<font>
						<Font name="Comic Sans MS" size="16.0" />
					</font>
				</Label>
				<Label alignment="CENTER" contentDisplay="CENTER" layoutX="350.0"
					layoutY="67.0" prefHeight="66.0" prefWidth="533.0" text="Withdraw"
					textAlignment="CENTER">
					<font>
						<Font name="Times New Roman" size="36.0" />
					</font>
				</Label>
				<GridPane fx:id="grid" layoutX="424.0" layoutY="172.0"
					prefHeight="126.0" prefWidth="385.0">
					<columnConstraints>
						<ColumnConstraints hgrow="SOMETIMES" maxWidth="184.0"
							minWidth="150.0" prefWidth="184.0" />
						<ColumnConstraints hgrow="SOMETIMES" maxWidth="200.0"
							minWidth="10.0" prefWidth="200.0" />
					</columnConstraints>
					<rowConstraints>
						<RowConstraints maxHeight="51.0" minHeight="10.0"
							prefHeight="30.0" vgrow="SOMETIMES" />
						<RowConstraints maxHeight="51.0" minHeight="10.0"
							prefHeight="30.0" vgrow="SOMETIMES" />
						<RowConstraints maxHeight="51.0" minHeight="10.0"
							prefHeight="30.0" vgrow="SOMETIMES" />
						<RowConstraints maxHeight="51.0" minHeight="10.0"
							prefHeight="30.0" vgrow="SOMETIMES" />
					</rowConstraints>
					<children>
						<Label prefHeight="30.0" prefWidth="185.0"
							style="-fx-background-color: #D4D4FC;" text="Cash To Be withdrawn :"
							GridPane.rowIndex="3">
							<font>
								<Font name="Arial Rounded MT Bold" size="14.0" />
							</font>
						</Label>
						<TextField fx:id="amount" prefHeight="30.0" prefWidth="150.0"
							promptText="INR" GridPane.columnIndex="1" GridPane.rowIndex="3" />
						<Label prefHeight="30.0" prefWidth="184.0"
							style="-fx-background-color: #D4D4FC;" text="Account Type : ">
							<font>
								<Font name="Arial Rounded MT Bold" size="14.0" />
							</font>
						</Label>
						<ChoiceBox fx:id="accountType" prefHeight="30.0"
							prefWidth="200.0" GridPane.columnIndex="1" />
						<Label prefHeight="30.0" prefWidth="184.0"
							style="-fx-background-color: #D4D4FC;" text="Available Bank Balance : "
							GridPane.rowIndex="2">
							<font>
								<Font name="Arial Rounded MT Bold" size="14.0" />
							</font>
						</Label>
						<Label fx:id="bankBalance" prefHeight="30.0" prefWidth="202.0"
							style="-fx-background-color: #FFF;" GridPane.columnIndex="1"
							GridPane.rowIndex="2" />
						<Label prefHeight="30.0" prefWidth="184.0"
							style="-fx-background-color: #D4D4FC;" text="Date : "
							GridPane.rowIndex="1">
							<font>
								<Font name="Arial Rounded MT Bold" size="14.0" />
							</font>
						</Label>
						<DatePicker fx:id="date" promptText="dd-MM-yyyy"
							stylesheets="/com/ccf/css/DatePicker.css" GridPane.columnIndex="1"
							GridPane.rowIndex="1" />
					</children>
				</GridPane>
				<Button layoutX="512.0" layoutY="330.0" mnemonicParsing="false"
					onAction="#clear" prefHeight="22.0" prefWidth="91.0"
					style="-fx-background-color: #0077cc; -fx-background-radius: 15;"
					text="Clear" textFill="WHITE">
					<cursor>
						<Cursor fx:constant="HAND" />
					</cursor>
					<font>
						<Font name="Arial Rounded MT Bold" size="14.0" />
					</font>
				</Button>

			</children>
		</AnchorPane>
	</children>
</VBox>
